{
  "name": "web-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:local": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "upload": "aws s3 sync out/ s3://viktor-livar-site-temp --delete",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E2J9TS3IT5CMO7 --paths \"/*\"",
    "deploy": "pnpm build && pnpm upload && pnpm invalidate"
  },
  "dependencies": {
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.5.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "typescript": "^5"
  }
}
